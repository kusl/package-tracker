                        document.body.style = "display:block";
                    }

                    try {
                        document.body.style = "display:none";
                    } catch (e) {
                    }
                    window.onload = cleanup;
                    window.clUnsupportedBrowser = true;
                }
                </script>
            <script src="//www.craigslist.org/js/general-concat.min.js?v=e9ae037676b2885c7a77adfdf33f8501" type="text/javascript" ></script>

