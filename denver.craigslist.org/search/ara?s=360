                    if (
                        !/\/\/.+\.craigslist\.org\/about\//.test(window.location.href)
                        &&
                        (!window.addEventListener || JSON.parse(JSON.stringify('x')) !== 'x')
                    ) {
                        throw "unsupported browser";
